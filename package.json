{
  "name": "remark-yaml-meta",
  "version": "1.0.0",
  "description": "Extract YAML front-matter from markdown.",
  "license": "MIT",
  "keywords": [
    "yaml",
    "options",
    "meta",
    "settings",
    "remark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/akagomez/remark-yaml-meta.git"
  },
  "bugs": "https://github.com/akagomez/remark-yaml-meta/issues",
  "author": "Chris Gomez <tituswormer@gmail.com> (http://akagomez.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "js-yaml": "^3.5.2"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.0",
    "nyc": "^10.0.0",
    "remark": "^7.0.0",
    "remark-cli": "^3.0.0",
    "remark-html": "^6.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.0.0",
    "unified": "^6.0.0",
    "xo": "^0.18.0"
  },
  "scripts": {
    "build-bundle": "browserify --no-builtins index.js -s remarkYAMLConfig > remark-yaml-meta.js",
    "build-mangle": "esmangle remark-yaml-meta.js > remark-yaml-meta.min.js",
    "build-md": "remark . --quiet --frail",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test.js",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "guard-for-in": "off",
      "no-eq-null": "off",
      "eqeqeq": "off"
    },
    "ignores": [
      "remark-yaml-meta.js",
      "remark-yaml-meta.min.js"
    ]
  },
  "remarkConfig": {
    "plugins": ["preset-wooorm"]
  }
}
