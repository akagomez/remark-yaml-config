{
  "name": "remark-yaml-config",
  "version": "2.0.0",
  "description": "Configure remark with YAML front-matter",
  "license": "MIT",
  "keywords": [
    "yaml",
    "options",
    "config",
    "settings",
    "remark"
  ],
  "dependencies": {
    "remark-yaml": "^2.0.0"
  },
  "repository": "wooorm/remark-yaml-config",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^12.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-github": "^2.0.0",
    "remark-lint": "^2.0.0",
    "remark-toc": "^2.0.0",
    "remark-usage": "^2.0.0",
    "tape": "^4.0.0"
  },
  "scripts": {
    "build-bundle": "browserify --no-builtins index.js -s remarkYAMLConfig > remark-yaml-config.js",
    "build-mangle": "esmangle remark-yaml-config.js > remark-yaml-config.min.js",
    "build-md": "remark . --quiet --frail",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
